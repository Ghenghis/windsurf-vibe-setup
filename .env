# Windsurf Vibe Setup Environment Configuration
# v4.3.0 Production Settings

# ðŸš¨ NO API KEYS ALLOWED! ONLY CLAUDE SUBSCRIPTION! ðŸš¨
# This system ONLY uses Claude subscription ($20/month)
# NEVER add API keys - they will be BLOCKED!

# Claude Subscription Token (NOT API KEY!)
# Get this by running: claude setup-token
CLAUDE_TOKEN=

# LLM Providers (Local Only)
OLLAMA_URL=http://localhost:11434
LMSTUDIO_URL=http://localhost:1234

# Vector Database
CHROMADB_URL=http://localhost:8000
CHROMADB_COLLECTION=windsurf-vibe

# Cache & Queue
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# Database
POSTGRES_URL=postgresql://windsurf:windsurf@localhost:5432/windsurf_vibe
POSTGRES_SSL=false

# Monitoring
HEALTH_DASHBOARD_PORT=9090
METRICS_PORT=9091
LOG_LEVEL=info

# Open Interpreter Configuration
INTERPRETER_MODEL=ollama/qwen2.5-coder:32b
INTERPRETER_SAFE_MODE=ask
INTERPRETER_CONTEXT_WINDOW=32768
INTERPRETER_AUTO_RUN=false
INTERPRETER_LOCAL=true

# Roo-Cline Integration
ROO_CODE_PATH=~/.roo-code
ROO_CUSTOM_MODES_PATH=~/.roo-code/custom-modes
ROO_DEFAULT_MODEL=qwen2.5-coder:32b

# Security
JWT_SECRET=your-secret-key-change-this
ENCRYPTION_KEY=your-encryption-key-change-this
API_RATE_LIMIT=100

# Feature Flags
ENABLE_HIVE_MIND=true
ENABLE_MULTI_AGENT=true
ENABLE_OPEN_INTERPRETER=true
ENABLE_MEMORY_SYSTEM=true
ENABLE_HEALTH_DASHBOARD=true

# Performance
MAX_CONCURRENT_AGENTS=10
MAX_SWARM_SIZE=50
TASK_QUEUE_SIZE=1000
MEMORY_CACHE_SIZE=100

# Development
NODE_ENV=development
DEBUG=false
