{
  "ignorePatterns": [
    {
      "pattern": "^https://github.com/Ghenghis/windsurf-vibe-setup/blob"
    },
    {
      "pattern": "localhost"
    },
    {
      "pattern": "127.0.0.1"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 301, 302, 307, 308]
}
